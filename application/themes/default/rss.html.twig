{% block content %}<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0"
	xmlns:content="http://purl.org/rss/1.0/modules/content/"
	xmlns:wfw="http://wellformedweb.org/CommentAPI/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:atom="http://www.w3.org/2005/Atom"
	xmlns:sy="http://purl.org/rss/1.0/modules/syndication/"
	xmlns:slash="http://purl.org/rss/1.0/modules/slash/"
	>

<channel>
	<title>Life Happens</title>
	<atom:link href="{{ staff.feed_url }}" rel="self" type="application/rss+xml" />
	<link>{{ staff.blog_url() }}</link>
	<description>{{ staff.bio }}</description>
	<lastBuildDate>
		{{ staff.latest_articles(1).get().created_at|date('r') }}
	</lastBuildDate>
	<language>en-US</language>
	<sy:updatePeriod>hourly</sy:updatePeriod>
	<sy:updateFrequency>1</sy:updateFrequency>
	<generator>http://staff.informatics.uii.ac.id</generator>
	
	{% for article in staff.latest_articles(10).get() %}
	<item>
		<title>{{ article.title }}</title>
		<link>{{ site_url(article.url()) }}</link>
		<pubDate>{{ article.created_at|date('r') }}</pubDate>
		<dc:creator>{{ staff.fullname }}</dc:creator>

		<description><![CDATA[Main ular-ularan di Google. Hoo~ sepertinya tahun depan tahun ular.]]></description>

	</item>
	{% endfor %}
		
</channel>
</rss>
{% endblock %}